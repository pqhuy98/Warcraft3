{
  "compilerOptions": {
    "baseUrl": "./src",
    "allowJs": true,
    "target": "es2017",
    "lib": [
      "es2019"
    ],
    "moduleResolution": "node",
    "paths": {
      "*": [
        "./*",
        "../node_modules/*/src/index",
        "../node_modules/*/lib/index",
        "../node_modules/*/index",
        "../node_modules/*/dist/index",
        "../node_modules/*"
      ]
    },
    "plugins": [
      {
        "transform": "war3-transformer",
        "mapDir": "D:/Projects/Warcraft3/gold-rush/maps/emeraldgardens.w3x",
        "entryFile": "D:/Projects/Warcraft3/gold-rush/src/main.ts",
        "outputDir": "D:/Projects/Warcraft3/gold-rush/dist/emeraldgardens.w3x"
      }
    ],
    "types": [
      "@typescript-to-lua/language-extensions",
      "lua-types/core/coroutine",
      "lua-types/core/global",
      "lua-types/core/math",
      "lua-types/core/metatable",
      "lua-types/core/modules",
      "lua-types/core/string",
      "lua-types/core/table",
      "lua-types/core/os",
      "lua-types/special/5.3",
      "war3-types-strict/1.33.0"
    ],
    "strictNullChecks": false
  },
  "include": [
    "src",
    "war3map.d.ts"
  ],
  "exclude": [],
  "tstl": {
    "luaTarget": "5.3",
    "noHeader": true,
    "luaLibImport": "require",
    "noImplicitSelf": true,
    "luaBundle": "dist/tstl_output.lua",
    "luaBundleEntry": "./src/main.ts",
    "luaPlugins": [
      {
        "name": "tstl-doender-plugin"
      }
    ]
  }
}